{
    "name": "git-jiggy",
    "description": "A fresh collection of node git utilities.",
    "version": "1.0.0",
    "author": {
        "name": "Michael Novotny",
        "email": "manovotny@gmail.com",
        "url": "https://manovotny.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manovotny/git-jiggy.git"
    },
    "keywords": [
        "add",
        "amend",
        "commit",
        "date",
        "git",
        "modified",
        "node",
        "staged",
        "utilities",
        "utility"
    ],
    "engines": {
        "node": ">=8.6.0",
        "yarn": ">=1.0.0"
    },
    "main": "index.js",
    "scripts": {
        "lint": "eslint . --ext .js --ext .json",
        "prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
        "test": "yarn lint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,md,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "@sindresorhus/is": "0.15.0",
        "execa": "1.0.0"
    },
    "devDependencies": {
        "eslint": "4.19.1",
        "eslint-config-get-off-my-lawn-prettier": "1.0.0",
        "husky": "1.3.1",
        "lint-staged": "8.1.5",
        "prettier": "1.16.4"
    }
}
